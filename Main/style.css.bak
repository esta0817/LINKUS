.image{
    position:absolute;
    margin-left: 30%;
    -webkit-animation: mymove ; /* Safari 4.0 - 8.0 */
    -webkit-animation-duration: 0.5s; /* Safari 4.0 - 8.0 */
   -webkit-animation-fill-mode: forwards; 

}
@-webkit-keyframes mymove {
    from {top: 10%;}
    to {top: 30%}
}

.smallpoi{
    opacity: 0;
